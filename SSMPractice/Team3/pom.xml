	<build>
		<finalName>shop</finalName>
		<plugins>
			<plugin>
				<groupId>org.apache.tomcat.maven</groupId>
				<artifactId>tomcat7-maven-plugin</artifactId>
				<version>2.2</version>
				<configuration>
					<url>http://localhost:8888/manager/text</url>
					<!-- <path>/</path> -->
					<username>admin</username>
					<password>123</password>
				</configuration>
			</plugin>
		</plugins>
